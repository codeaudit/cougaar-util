<?xml version="1.0"  encoding="US-ASCII"?>

<!DOCTYPE Policy SYSTEM "Policy.ldm.dtd" []>

<!-- Inventory Policy for CORPS-RPRPTCO, et al: Corps-Level Materiel Supply units -->

<Policies>

 <Policy name="ConsumableInventoryPolicy" type="org.cougaar.glm.plugins.inventory.InventoryPolicy">
    <RuleParam name="ResourceType">
      	<String value ="Consumable">
	</String>
    </RuleParam>
    <RuleParam name="DaysOnHand">
	<Integer value="20" min="-1" max="90"> 
	</Integer>
    </RuleParam>
    <RuleParam name="DaysForward">
        <Integer value="7" min="0" max="30">
	</Integer>
    </RuleParam>
    <RuleParam name="DaysBackward">
        <Integer value="0" min="0" max="30">
	</Integer>
    </RuleParam>
    <RuleParam name="GoalLevelMultiplier">
        <Double value="2" min="1.0" max="30">
	</Double>
    </RuleParam>
    <RuleParam name="WithdrawSwitchoverDay">
        <Integer value="100" min="0" max="300">
        </Integer>
    </RuleParam>
    <RuleParam name="RefillSwitchoverDay">
        <Integer value="100" min="0" max="300">
        </Integer>
    </RuleParam>
    <RuleParam name="TurnOffProjections">
        <Boolean value="false">
        </Boolean>
    </RuleParam>
  </Policy>

</Policies>
