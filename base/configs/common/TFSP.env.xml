<?xml version="1.0"  encoding="US-ASCII"?>

<!DOCTYPE tasklist SYSTEM "TOPSParam.env.dtd" []>

<Parameter_List>
  <!-- shows debug output -->
  <Parameter name="ExtraOutput"      	type="boolean" value="false" />
  <!-- shows extremely detailed debug output -->
  <Parameter name="ExtraExtraOutput" 	type="boolean" value="false" />

  <!-- the parameters control the buffering of input tasks  -->
  <!-- maxSize determines when the task buffer is full.  When this numer is reached, -->
  <!-- the plugin begins working on the buffered tasks. -->
  <!-- maxTime says 'even if less than MaxSize tasks have arrived, being processing anyway -->
  <Parameter name="MaxSize"          	type="long"    value="800" />
  <Parameter name="MaxTime"          	type="long"    value="120" />

  <!-- don't go through web server to run Vishnu scheduler -->
  <!-- yes, it's faster, but you can't see the results -->
  <Parameter name="runInternal" type="boolean" value="false"/>

  <!-- only show XML from ALP, don't send scheduling request -->
  <Parameter name="showALPXML"     type="boolean" value="false" />

  <!-- show XML from format transform, but won't interfere with scheduling request -->
  <!-- (this xml is massaged a bit before being sent to VISHNU) -->
  <Parameter name="showFormatXML"  type="boolean" value="false" />

  <!-- show XML from data transform, but won't interfere with scheduling request -->
  <!-- (this xml is massaged a bit before being sent to VISHNU) -->
  <Parameter name="showDataXML"    type="boolean" value="false" />

  <!-- dump to stdout xml that goes to scheduler -->
  <Parameter name="testing"     type="boolean" value="false" />

  <!-- show timing information -->
  <Parameter name="showTiming"     type="boolean" value="false" />

  <!-- make implied tasks that come before and after main task -->
  <Parameter name="makeSetupAndWrapupTasks" type="boolean" value="true" />

  <!-- COUGAAR-Vishnu bridge waits waitTime seconds between polls, and -->
  <!-- will poll only maxWaitCycles times -->
  <Parameter name="waitTime"    type="long"    value="5" /> <!-- seconds -->
  <Parameter name="maxWaitCycles"       type="int"     value="60" />

  <!-- uses the first X tasks to determine the format of all input tasks -->
  <!-- interacts with MaxSize parameter -->
  <Parameter name="firstTemplateTasks"    type="int"    value="5000" />

  <!-- The time window that the problem will take place in. -->
  <!-- Sets initial result display scaling -->
  <Parameter name="vishnuEpochStartTime"  type="String"  value="2001-05-01 00:00:00" />
  <Parameter name="vishnuEpochEndTime"    type="String"  value="2002-01-01 00:00:00" />

  <!-- settings for alp-107.org.cougaar.isotic.org -->
  <Parameter name="hostName"    type="String"  value="alp-107.org.cougaar.isotic.org" />
  <Parameter name="phpPath"     type="String"  value="/~demo/TOPS/vishnu/php/" />
    <!-- these are root and the empty string on alp-107 -->
  <Parameter name="user"        type="String"  value="root" />
  <Parameter name="password"    type="String"  value="nopassword" />

  <!-- settings for dante.bbn.com -->
<!--
  <Parameter name="hostName"    type="String"  value="dante" />
  <Parameter name="phpPath"     type="String"  value="/~dmontana/vishnu/" />
