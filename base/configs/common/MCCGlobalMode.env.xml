<?xml version="1.0"  encoding="US-ASCII"?>

<!DOCTYPE tasklist SYSTEM "TOPSParam.env.dtd" []>

<Parameter_List>
  <Parameter name="ExtraOutput"      type="boolean" value="true" />
  <Parameter name="ExtraExtraOutput" type="boolean" value="true" />
  <Parameter name="DEBUG_GSS" type="boolean" value="true" />

  <Parameter name="SleepTime" type="long" value="5" />
  <Parameter name="MaxSize" type="long" value="1" />
