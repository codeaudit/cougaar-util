<?xml version="1.0" encoding="UTF-8"?>
<xml>

  <!-- this is for querying a cluster about its location schedule -->
  <glquery>
    <keyname>ORG_LOC</keyname>
    <predicate>
      <instanceof>org.cougaar.domain.glm.ldm.asset.Organization</instanceof>
    </predicate>
    <adapter>org.cougaar.lib.uiframework.ui.orglocation.psp.xmlservice.LocationScheduleToXml</adapter>
  </glquery>

  <!-- this is for querying a cluster about its relationship schedule -->
  <glquery>
    <keyname>ORG_RELS</keyname>
    <predicate>
      <instanceof>org.cougaar.domain.glm.ldm.asset.Organization</instanceof>
    </predicate>
    <adapter>org.cougaar.lib.uiframework.ui.orgui.OrgSubAdapter</adapter>
  </glquery>

  <!-- this is for querying a cluster about its demand -->
  <glquery>
    <keyname>QUERY_DEMAND</keyname>
    <predicate>
      <instanceof>org.cougaar.domain.planning.ldm.plan.Task</instanceof>
    </predicate>
    <adapter>mil.darpa.log.alpine.blackjack.assessui.society.DemandQueryAdapter</adapter>
  </glquery>

  <!-- this is for querying a cluster about its inventory -->
  <glquery>
    <keyname>QUERY_INVENTORY</keyname>
    <predicate>
      <instanceof>org.cougaar.domain.planning.ldm.asset.Asset</instanceof>
    </predicate>
    <adapter>mil.darpa.log.alpine.blackjack.assessui.society.InventoryQueryAdapter</adapter>
  </glquery>

  <!-- this is for querying a cluster about its list of dueouts -->
  <glquery>
    <keyname>QUERY_DUEOUT</keyname>
    <predicate>
      <instanceof>org.cougaar.domain.planning.ldm.asset.Asset</instanceof>
    </predicate>
    <adapter>mil.darpa.log.alpine.blackjack.assessui.society.DueOutQueryAdapter</adapter>
  </glquery>

  <!-- this is for querying a cluster about its due ins -->
  <glquery>
    <keyname>QUERY_DUEIN</keyname>
    <predicate>
      <instanceof>org.cougaar.domain.planning.ldm.plan.Task</instanceof>
    </predicate>
    <adapter>mil.darpa.log.alpine.blackjack.assessui.society.DueInQueryAdapter</adapter>
  </glquery>

  <!-- this is for querying a cluster about its list of target levels -->
  <glquery>
    <keyname>QUERY_TARGETLEVEL</keyname>
    <predicate>
      <instanceof>org.cougaar.domain.planning.ldm.asset.Asset</instanceof>
    </predicate>
    <adapter>mil.darpa.log.alpine.blackjack.assessui.society.TargetLevelQueryAdapter</adapter>
  </glquery>

  <!-- this is for querying a cluster about its list of dueouts -->
  <glquery>
    <keyname>QUERY_CRITICALLEVEL</keyname>
    <predicate>
      <instanceof>org.cougaar.domain.planning.ldm.asset.Asset</instanceof>
    </predicate>
    <adapter>mil.darpa.log.alpine.blackjack.assessui.society.CriticalLevelQueryAdapter</adapter>
  </glquery>
</xml>

