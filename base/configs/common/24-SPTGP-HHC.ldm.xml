<AssetList>

  <!-- ::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: -->
  <!-- :::                    First the Prototypes                     ::: -->
  <!-- ::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: -->

  &ROWPU-PROTOTYPE;
  &WaterTrailer400-PROTOTYPE;
  &WaterTanker3000-PROTOTYPE;
  &WaterTanker5000-PROTOTYPE;

  <!-- ::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: -->
  <!-- :::                      Now the Instances                      ::: -->
  <!-- ::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: -->

  <instance prototype="ROWPU" id="CombinedROWPUoutput">
    <schedule end="01/05/11" start="06/01/00"/>
  </instance>

  <!-- :::::::::::::::::::::::::::::::::::::::::::::::::: -->
  <!-- :::       M149 Sources                         ::: -->
  <!-- :::                                            ::: -->
  <!-- :::  12 from Bde's 3 Combat Bns                ::: -->
  <!-- :::  4 from Bde's FA Bn                        ::: -->
  <!-- :::  4 from Bde's Eng Bn                       ::: -->
  <!-- :::  4 from Bde's FSB                          ::: -->
  <!-- :::                                            ::: -->
  <!-- :::  Bdes are 1st@3ID, 2nd@3ID, & 2nd@2ID      ::: -->
  <!-- :::                                            ::: -->
  <!-- :::  24 * 3 Bdes = 72                          ::: -->
  <!-- :::::::::::::::::::::::::::::::::::::::::::::::::: -->

  <instance prototype="WaterTrailer400" id="WaterTrailer400" quantity="48">
    <schedule end="01/05/11" start="06/01/00"/>
  </instance>

  <!-- :::::::::::::::::::::::::::::::::::::::::::::::::: -->
  <!-- :::      SMFT Sources                          ::: -->
  <!-- :::                                            ::: -->
  <!-- :::  2 from 3ID DISCOM 703 MSB                 ::: -->
  <!-- :::  2 from 2ID DISCOM 296 SB                  ::: -->
  <!-- :::  4 from 24th SptGrp 226 QM Supply Co       ::: -->
  <!-- :::::::::::::::::::::::::::::::::::::::::::::::::: -->

  <instance prototype="WaterTanker3000" id="WaterTanker3000" quantity="2">
    <schedule end="01/05/11" start="06/01/00"/>
  </instance>
  <instance prototype="WaterTanker5000" id="WaterTanker5000" quantity="10">
    <schedule end="01/05/11" start="06/01/00"/>
  </instance>

</AssetList>
