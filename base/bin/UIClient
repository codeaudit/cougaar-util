#!/bin/csh -f

source $COUGAAR_INSTALL_PATH/bin/setlibpath.csh

set MYCLASSES="ui.planviewer.XMLUIDataClient"


set osargs=""
set os=`uname`
if ("$os" == "Linux") then
  set osargs="-green"
endif

if ($?COUGAAR_DEV_PATH) then
    echo java $osargs -classpath $LIBPATHS $MYCLASSES
endif

java $osargs -classpath $LIBPATHS $MYCLASSES

